page {
    background-color: #dedede;
  }

  .my_collect{
    display: flex;
    flex-direction: column;
    align-items: center;
    font-size: 60rpx;
    background-color: #546dfa;
    color: #fff;
  }
  

  .dustbin{
    padding: 0;
    position: absolute;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 10rpx;
    z-index:2;
  }
  .dustbin:not([size="mini"]) {
    width: 150rpx;
    height: 150rpx;
  }

  .dustbin_image{
    margin: 0;
    width:120rpx;
    height: 120rpx;
    border: none;
   }

  .collect-table-wrap{
    position: relative;
    width: 668rpx;
    height: 1200rpx;
    left: 50%;
    margin-top: 30rpx;
    margin-left: -334rpx;
    top: 0rpx;
    overflow-y: scroll;
    overflow-x: hidden;
  }
  /* 表格代码   */
  .table{
    border-width: 0;
    border-right: 0;
    border-bottom: 0;
    width: 98%;
    margin-left: 1%;
  }
  .addin{
    margin-left: 50rpx;
    margin-right: 50rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
  }
  .addin_name{
    font-size: 40rpx;
    margin: 50rpx;
    color: #161616;
    width: 40%;
  }
  .addin_image{
    margin: 0;
    width: 100rpx;
    height: 100rpx;
    border: none;
   }
  .is_favorite{
    margin: 0;
    width: 80rpx;
    height: 80rpx;
    border: none;
  }


  .search_con .search_input{
    position: absolute;
    border: 1px solid rgb(214, 211, 211);
    width: 253px;
    height: 45px;
    border-radius: 100px;
    font-size: 17px;
    padding-left: 25px;/*此处要用padding-left才可以把光标往右移动15像素，不可以用text-indent*/
    color: #333;
    display: inline-block;
   }
   
   .search_con .search_btn{
     position: absolute;
   right: 25px;
   position: absolute;
   top: 18.9px;
    width: 85px;
    height: 43px;
    background: #56b273;
    color: #fff;
    border-radius: 50px;
    display: inline-block;
   }