<!--pages/ahead/ahead.wxml-->
<view class='image'>
<image src="/pages/logo.jpg" class="size"></image>
</view>
<view class="searchbox">
<input placeholder="请输入搜索关键字" bindinput="getInput" value="{{val}}"></input>
<icon type="search" wx:if="{{isSearch}}"></icon>
<icon type="clear" wx:if="{{isClear}}" bindtap="clearTap"></icon>
</view>